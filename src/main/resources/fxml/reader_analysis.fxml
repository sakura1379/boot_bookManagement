<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="boot_bookmanage.java.controller.ReaderAnalysisController" styleClass="default-theme">
    <GridPane hgap="40" vgap="40" AnchorPane.topAnchor="20" styleClass="sameDefault-theme">
        <Button fx:id="returnButton" styleClass="sameDefault-theme,btn-radius" onAction="#logout" GridPane.columnIndex="0" GridPane.rowIndex="0" />
        <StackPane fx:id="departmentPieChart" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
        <StackPane fx:id="rolePieChart" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    </GridPane>
</AnchorPane>
